thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       tabulated;
    thermo          hTabulated;
    equationOfState rhoTabulated;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        molWeight       120.914;
    }

    #include "$FOAM_TUTORIALS/resources/thermoData/wallBoiling-liquid"
